<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
  <display-name>JSPDemo</display-name>
  
  <servlet>
  <servlet-name>year</servlet-name>
  <servlet-class>com.register.controller.GetYear</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>year</servlet-name>
  <url-pattern>/GetYear</url-pattern>
  </servlet-mapping>
  
   <servlet>
  <servlet-name>register</servlet-name>
  <servlet-class>com.register.controller.RegisterStudent</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>register</servlet-name>
  <url-pattern>/RegisterStudent</url-pattern>
  </servlet-mapping>
  
  <welcome-file-list>
    <welcome-file>Welcome.html</welcome-file>
    <welcome-file>registeration.jsp</welcome-file>
  </welcome-file-list>
</web-app>